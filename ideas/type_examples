type Person: struct {
    age: Int,
    name: String,
    gender: enum {
        Male,
        Female
    },
    nickname: enum {
        Some: struct {String},
        None
    },
}

type Person: {
    age: Int,
    name: String,
    gender: {Male | Female},
    nickname: {
      Some: {String}
    | None
    },
}

type Person: [
    age: Int,
    name: String,
    gender: {Male, Female},
    nickname: {
      Some: [String],
      None
    },
]

