// Could use #A instead of 'A

function elementInArray(move elem: 'T, arr: Array('T)) -> Option('T) {
    for (i = 0, arr.length) {
        if arr[i] == elem {
            return elem;
        }
    }

    return None;
}

// Where statement
function elementInArray(move elem: 'A, arr: 'B) -> 'A where
    'A: Simple,
    'B: Array('A) {

    for (i = 0, arr.length) {
        if arr[i] == elem {
            return elem;
        }
    }

    return None;
}

