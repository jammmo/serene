type Person is struct {
    age: int,
    name: String,
    gender: enum {
        Male,
        Female
    },
    nickname: enum {
        Some (String),
        None
    },
}

type Point3D is tuple {
    int,
    int,
    int
}

type Address is tuple {
    int,    // House number
    String, // Street name
}

type RainbowColors is enum {
    Red,
    Orange,
    Yellow,
    Green,
    Blue,
    Purple
}