function binarySearch(vec: Vector of Int64, element: X) on (type X) -> X {
    var low = 0
    var high = vec.length()
    while (low < high) {
        var mid = (low + high) / 2  // integer division
        if (vec[mid] < element) {
            set low = mid + 1
        } else {
            set high = mid
        }
    }
    return low
}

