function elementwise3DAdd(move a: Vector of Vector of Vector of Int, move b: Vector of Vector of Vector of Int) -> Vector of Vector of Vector of Int {
    for (i = 0, a.length()) {
        for (j = 0, a[i].length()) {
            for (k = 0, a[i][j].length()) {
                set a[i][j][k] = a[i][j][k] + b[i][j][k]
            }
        }
    }
    return a
}

function main() {
    var a = Vector( Vector(Vector(1, 2), Vector(-3, 6)) , Vector(Vector(9, 32), Vector(29, -1)) )
    var b = Vector( Vector(Vector(3, 2), Vector(7, -2)) , Vector(Vector(-5, -28), Vector(-25, 5)) )

    print "  A  : ", a
    print "  B  : ", b
    print ""

    var c = elementwise3DAdd(move a, move b)
    print "A + B: ", c
}