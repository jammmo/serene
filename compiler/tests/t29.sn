function main() {
    var a = "hello"

    // Comment out all but one section

    // Should not compile
    if (fn_discard(move a) == 0) {
        print 0
    } else {
        run fn_print(a)
    }

    // Should not compile
    if (fn_print(a) == 0) {
        print 0
    } else {
        run fn_discard(move a)
    }
    run fn_print(a)

    // Should not compile
    if (0 < 10) {
        print 0
    } elseif (fn_discard(move a) == 0) {
        run fn_print(a)
    }

    // Should compile
    if (1 == 20) {
        run fn_discard(move a)
    } elseif (1 == 30) {
        print 0
    } elseif (fn_print(a) == 0) {
        run fn_discard(move a)
    } elseif (fn_discard(move a) == 0) {
        print 1
    } else {
        print 2
    }

    // Should not compile
    while (fn_discard(move a) == 0) {
        print 0
    }

    // Should not compile
    var i = 0
    while (i < 100) {
        run fn_discard(move a)
        set i += 1
    }

    // Should not compile
    for (x in mut_with_array(mutate a)) {
        print x
    }
}


function fn_print(thing: String) -> Int {
    print thing
    return 0
}

function fn_discard(move thing: String) -> Int {
    return 0
}

function mut_with_array(mutate thing: String) -> Array{Char} {
    set thing[0] = thing[1]
    return Array(thing[0])
}